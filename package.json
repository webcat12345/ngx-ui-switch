{
  "name": "ngx-ui-switch",
  "version": "13.0.1",
  "description": "switch button for angular4",
  "bugs": "https://github.com/webcat12345/ngx-ui-switch/issues",
  "homepage": "https://github.com/webcat12345/ngx-ui-switch#readme",
  "repository": "https://github.com/webcat12345/ngx-ui-switch.git",
  "license": "MIT",
  "author": "webcat12345",
  "private": true,
  "keywords": [
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "ngx",
    "ng2",
    "uiswitch",
    "switch",
    "ui"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng-packagr -p src/lib/package.json",
    "postbuild": "ts-node -P tsconfig.build.json build.ts",
    "build:demo": "rimraf demo && ng build --aot --configuration production --base-href=/ngx-ui-switch/demo/ --output-path=demo",
    "release": "cd dist && yarn publish",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "cleanup": "rimraf build dist waste deploy demo docs",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=protractor-ci.conf.js",
    "typedoc": "typedoc --options typedoc.json src/lib/*.ts && ts-node -P tsconfig.build.json docs.ts"
  },
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.0",
    "@angular-eslint/builder": "^13.0.0",
    "@angular-eslint/eslint-plugin": "^13.0.0",
    "@angular-eslint/eslint-plugin-template": "^13.0.0",
    "@angular-eslint/schematics": "^13.0.0",
    "@angular-eslint/template-parser": "^13.0.0",
    "@angular/cli": "^13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "all-contributors-cli": "^5.10.0",
    "eslint": "^8.2.0",
    "fs-extra": "^10.0.0",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "sass": "^1.35.1",
    "ts-node": "^10.2.0",
    "tsickle": "^0.39.0",
    "typedoc": "^0.14.2",
    "typescript": "~4.5.4"
  }
}
